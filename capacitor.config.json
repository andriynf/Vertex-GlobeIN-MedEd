{
  "appId": "com.bhoward3.globeinmeded",
  "appName": "Vertex GlobeIN MedEd",
  "webDir": "dist",
  "server": {
    "androidScheme": "https"
  },
  "plugins": {
    "SplashScreen": {
      "1launchShowDuration": 3000,
      "launchAutoHide": true,
      "1launchFadeOutDuration": 3000,
      "1backgroundColor": "#ffffffff",
      "1androidSplashResourceName": "splash",
      "1androidScaleType": "CENTER_CROP",
      "1showSpinner": true,
      "1androidSpinnerStyle": "large",
      "1iosSpinnerStyle": "small",
      "1spinnerColor": "#999999",
      "splashFullScreen": true,
      "splashImmersive": true,
      "1layoutName": "launch_screen",
      "useDialog": true
    }
  }
}
